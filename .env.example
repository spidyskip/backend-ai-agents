# Environment
ENVIRONMENT=development

# Database configuration
DATABASE_TYPE=sqlite  # Options: sqlite, mongodb
SQLITE_URL=sqlite:///./agents.db
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/agents_db
MONGODB_DB_NAME=agents_db

# Document storage configuration
DOCUMENT_STORAGE_TYPE=local  # Options: local, s3
LOCAL_DOCUMENT_DIR=docs

# S3 configuration
USE_S3_STORAGE=false
S3_BUCKET_NAME=your-bucket-name
S3_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key

# API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
SERPER_API_KEY=your_serper_api_key_here
GOOGLE_API_KEY=your_google_api_key_here

# Default LLM model
DEFAULT_LLM_MODEL=gpt-4o

# Logging configuration
LOG_LEVEL=INFO

